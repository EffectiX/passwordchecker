{
    "name": "morales2k/laravel-password-health-checker",
    "description": "Laravel Password Health Checker is a package that intends to provide a validation rule to score how secure a password string is when validating password strings on registration forms.",
    "keywords": [
        "J.Morales",
        "laravel",
        "laravel-password-health-checker"
    ],
    "homepage": "https://github.com//laravel-password-health-checker",
    "license": "MIT",
    "authors": [
        {
            "name": "J.Morales",
            "email": "jlm787@proton.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 | ^8.0 | ^8.1 | ^8.2 | ^8.3 | ^8.4",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^10.0||^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "larastan/larastan": "^2.9",
        "orchestra/testbench": "^9.0.0||^8.22.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "EffectiX\\LPHC\\": "src/",
            "EffectiX\\LPHC\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EffectiX\\LPHC\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EffectiX\\LPHC\\LPHCServiceProvider"
            ],
            "aliases": {
                "LPHC": "EffectiX\\LPHC\\Facades\\LPHC"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}